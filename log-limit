#!/bin/bash
echo "----------------------- พัฒนาโดย SUPERNET-VPN.COM -----------------------"
echo " "
echo "===============================================";
echo " ";
if [ -e "/root/log-limit.txt" ]; then
echo "ผู้ใช้ที่ขีดฆ่าโดยผู้ใช้มีขีด จำกัด";
echo "เวลา - ชื่อผู้ใช้ - จำนวน"
echo "-------------------------------------";
cat /root/log-limit.txt
else
echo " ไม่มีผู้ใช้รายใดกระทำผิดกฎหมาย "
echo " หรือ"
echo " สคริปยังไม่ทำงาน"
fi
echo " ";
echo "===============================================";
echo " ";
echo " ";
